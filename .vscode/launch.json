  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      //"program": "${workspaceFolder}/build/test/UKFClassTests",
      "program": "${workspaceFolder}/build/src/UnscentedKF",
      "args": ["${workspaceFolder}/data/obj_pose-laser-radar-synthetic-input.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}